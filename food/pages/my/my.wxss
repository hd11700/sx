.card{
 
  background-image: url('data:image/png;base64,'); 
	background-repeat:no-repeat;
	background-size:100% 100%;
  -moz-background-size:100% 100%;
  margin: auto;
  position: relative;
  top: -150rpx;
  height: 450rpx;
  width: 90%;
}

.card_container{
height: 260rpx;
}

.card .avatar{
  position: absolute;
  left: 40rpx;
  top: 40rpx;
  opacity: 0.8;
}
.userinfo-avatar {
  overflow: hidden;
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}
.hasNotUserInfo{
  position: absolute;
  left: 80rpx;
  top: 100rpx;
  width: 300rpx;
  height: 100rpx;
  background-color:#729b89;
  border-radius: 999px;
  color: white;
  text-align: center;
  line-height: 100rpx;
  opacity: 0.8;
}
.card .nick{
  position: absolute;
  left: 80rpx;
  top: 250rpx;
  opacity: 0.8;
}

.card .body{
  position: absolute;
 right: 80rpx;
 top: 80rpx;
 font-weight: bold;
 font-size: 40rpx;
 letter-spacing: 5rpx;
 opacity: 0.8;
}

.card .body_info{
  position: absolute;
  right: 40rpx;
  top: 150rpx;
  font-size: 24rpx;
  letter-spacing: 5rpx;
  opacity: 0.8;
  width: 200rpx;
  text-align: left;
}

.card .time{
  position: absolute;
  left: 80rpx;
  bottom:50rpx;
  font-size: 30rpx;
  color: #e8cc9c;
}

.more{
  color: #e8cc9c;
  font-size: 30rpx;
  margin-left: 15%;
  height: 50rpx;
  line-height: 50rpx;
  letter-spacing: 5rpx;
  margin-top: 40rpx;
}

